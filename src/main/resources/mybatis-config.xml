<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias alias="board" type="com.example.alstarboard.domain.Board"/>
        <typeAlias alias="user" type="com.example.alstarboard.domain.User"/>
        <typeAlias alias="image" type="com.example.alstarboard.domain.Image"/>
        <typeAlias alias="reply" type="com.example.alstarboard.domain.Reply"/>
    </typeAliases>
</configuration>